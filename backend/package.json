{
	"name": "ippl-backend",
	"version": "2.1.0",
	"description": "Backend para el sistema administrativo del IPPL",
	"main": "src/index.js",
	"scripts": {
		"start": "node src/index.js",
		"lint": "eslint src/**/*.js",
		"lint:fix": "eslint src/**/*.js --fix",
		"db:create": "sequelize-cli --config config/config.js db:create",
		"db:migrate": "sequelize-cli --config config/config.js db:migrate",
		"db:seed:all": "sequelize-cli --config config/config.js db:seed:all",
		"dev": "nodemon src/index.js",
		"setup": "node src/setup.js",
		"db:reset": "sequelize-cli --config config/config.js db:drop && npm run db:create && npm run db:migrate && npm run db:seed:all"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"cors": "^2.8.5",
		"dotenv": "^16.6.1",
		"express": "^4.18.2",
		"express-rate-limit": "^8.2.1",
		"funciones-basicas": "^2.1.14",
		"helmet": "^8.1.0",
		"joi": "^18.0.1",
		"jsonwebtoken": "^9.0.0",
		"multer": "^1.4.5-lts.1",
		"mysql2": "^3.14.3",
		"nodemailer": "^6.10.1",
		"sequelize": "^6.37.7",
		"uuid": "^11.1.0",
		"winston": "^3.18.3",
		"winston-daily-rotate-file": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.34.0",
		"eslint": "^9.34.0",
		"globals": "^16.3.0",
		"nodemon": "^2.0.22",
		"sequelize-cli": "^6.6.3"
	}
}
